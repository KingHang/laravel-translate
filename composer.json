{
    "name": "kinghang/laravel-translate",
    "description": "集成百度翻译、有道翻译、谷歌翻译的laravel扩展包",
    "require": {
        "kinghang/translate": "^1.0",
        "illuminate/support": "^6|^7|^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.20"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "KingHang",
            "email": "863390956@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "KingHang\\LaravelTranslate\\": "./src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KingHang\\LaravelTranslate\\ServiceProvider"
            ],
            "aliases": {
                "Translate": "KingHang\\LaravelTranslate\\Translate"
            }
        }
    }
}
